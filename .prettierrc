{
  "arrowParens": "always",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "printWidth": 80,
  "proseWrap": "preserve",
  "tabWidth": 2,
  "singleQuote": false,
  "quoteProps": "as-needed",
  "trailingComma": "es5",
  "semi": true,
  "useTabs": false,
  "jsxSingleQuote": false,
  "jsxBracketSameLine": false,
  "requireConfig": false,
  "requirePragma": false,
  "htmlWhitespaceSensitivity": "css"
}
